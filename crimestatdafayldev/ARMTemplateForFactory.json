{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "crimestatdafayldev"
		},
		"crimestatdwhlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'crimestatdwhlinkedservice'"
		},
		"crimestatstacyllinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'crimestatstacyllinkedservice'"
		},
		"AzureKeyVaultcrimestatkevayllinkedservice_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://crimestatkevayldev.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Source-Country-Load-Staging')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_vxb",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "crimestat-sourcefiles/CrimeStat.Source.Countries/CrimeStat.Source.Countries/ISOcountries.csv"
							},
							{
								"name": "Destination",
								"value": "staging.Countries_untyped"
							}
						],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings",
									"skipLineCount": 0
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "name",
											"type": "String"
										},
										"sink": {
											"name": "name",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "alpha-2",
											"type": "String"
										},
										"sink": {
											"name": "alpha-2",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "alpha-3",
											"type": "String"
										},
										"sink": {
											"name": "alpha-3",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "country-code",
											"type": "String"
										},
										"sink": {
											"name": "country-code",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "iso_3166-2",
											"type": "String"
										},
										"sink": {
											"name": "iso_3166-2",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "region",
											"type": "String"
										},
										"sink": {
											"name": "region",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "sub-region",
											"type": "String"
										},
										"sink": {
											"name": "sub-region",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "intermediate-region",
											"type": "String"
										},
										"sink": {
											"name": "intermediate-region",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "region-code",
											"type": "String"
										},
										"sink": {
											"name": "region-code",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "sub-region-code",
											"type": "String"
										},
										"sink": {
											"name": "sub-region-code",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "intermediate-region-code",
											"type": "String"
										},
										"sink": {
											"name": "intermediate-region-code",
											"type": "String"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_vxb",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_vxb",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SourceDataset_vxb')]",
				"[concat(variables('factoryId'), '/datasets/DestinationDataset_vxb')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DestinationDataset_vxb')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "crimestatdwhlinkedservice",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "name",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "alpha-2",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "alpha-3",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "country-code",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "iso_3166-2",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "region",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "sub-region",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "intermediate-region",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "region-code",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "sub-region-code",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					},
					{
						"name": "intermediate-region-code",
						"type": "nvarchar",
						"precision": 255,
						"scale": 255
					}
				],
				"typeProperties": {
					"schema": "staging",
					"table": "Countries_untyped"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/crimestatdwhlinkedservice')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SourceDataset_vxb')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "crimestatstacyllinkedservice",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "ISOcountries.csv",
						"folderPath": "CrimeStat.Source.Countries/CrimeStat.Source.Countries",
						"container": "crimestat-sourcefiles"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/crimestatstacyllinkedservice')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureKeyVaultcrimestatkevayllinkedservice')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('AzureKeyVaultcrimestatkevayllinkedservice_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/crimestatdwhlinkedservice')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('crimestatdwhlinkedservice_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/crimestatstacyllinkedservice')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('crimestatstacyllinkedservice_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Trigger_vxb')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Source-Country-Load-Staging",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 1,
						"startTime": "2019-12-18T20:48:00.000Z",
						"timeZone": "UTC",
						"schedule": {
							"minutes": [
								0
							],
							"hours": [
								0
							]
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/Source-Country-Load-Staging')]"
			]
		}
	]
}